package com.haru.model;

public class Reply {
	
	private int rno;  // 댓글 번호
    private String comment;  // 댓글 내용
    
    public int getRno() {
        return rno;
    }

    public void setRno(int rno) {
        this.rno = rno;
    }

    public String getComment() {
        return comment;
    }

    public void setComment(String comment) {
        this.comment = comment;
    }

}
